import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int rev = 0;
        while(n!=0){
            rev = n%10;
            n=n/10;
            System.out.print(rev);
        }
    }
}

input:
98765

output:
56789
